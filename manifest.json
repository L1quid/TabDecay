{
  "manifest_version": 2,

  "name": "Tab Decay",
  "description": "A Chrome extension which bookmarks and closes tabs that have been unused for days.",
  "version": "1.0",

  "permissions": [
    "http://*/*",
    "storage",
    "tabs",
    "webNavigation",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "decaying_tab.js",
      "dt_base.js",
      "dt_boot.js",
      "dt_tabs.js",
      "dt_bookmarks.js",
      "background.js"
    ]
  }
}
